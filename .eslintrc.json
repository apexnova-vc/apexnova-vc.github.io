{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      // Custom rules or overrides
      "@typescript-eslint/no-unused-vars": [
          1,
          {
              "argsIgnorePattern": "_*"
          }
      ]
    },
    "env": {
      "browser": true,
      "es2021": true
    },
    "plugins": [
      "react",
      "@typescript-eslint",
      "react-hooks"
    ]
  }
  